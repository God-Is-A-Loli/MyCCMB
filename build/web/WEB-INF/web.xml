<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>endpoint.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BasicInfo</servlet-name>
        <servlet-class>endpoint.BasicInfo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>endpoint.Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Tobacco</servlet-name>
        <servlet-class>endpoint.Tobacco</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Alcohol</servlet-name>
        <servlet-class>endpoint.Alcohol</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Personal</servlet-name>
        <servlet-class>endpoint.Personal</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Family</servlet-name>
        <servlet-class>endpoint.Family</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Allergy</servlet-name>
        <servlet-class>endpoint.Allergy</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Medication</servlet-name>
        <servlet-class>endpoint.Medication</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MedicalCondition</servlet-name>
        <servlet-class>endpoint.MedicalCondition</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Surgery</servlet-name>
        <servlet-class>endpoint.Surgery</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Gynecology</servlet-name>
        <servlet-class>endpoint.Gynecology</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterNewUser</servlet-name>
        <servlet-class>endpoint.RegisterNewUser</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BasicInfo</servlet-name>
        <url-pattern>/BasicInfo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Tobacco</servlet-name>
        <url-pattern>/Tobacco</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Alcohol</servlet-name>
        <url-pattern>/Alcohol</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Personal</servlet-name>
        <url-pattern>/Personal</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Family</servlet-name>
        <url-pattern>/Family</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Allergy</servlet-name>
        <url-pattern>/Allergy</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Medication</servlet-name>
        <url-pattern>/Medication</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MedicalCondition</servlet-name>
        <url-pattern>/MedicalCondition</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Surgery</servlet-name>
        <url-pattern>/Surgery</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Gynecology</servlet-name>
        <url-pattern>/Gynecology</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterNewUser</servlet-name>
        <url-pattern>/RegisterNewUser</url-pattern>
    </servlet-mapping>
    
    <session-config>
        <session-timeout>
            3600
        </session-timeout>
    </session-config>
    <listener>
        <listener-class>listener.LifeCycleListener</listener-class>
    </listener>
</web-app>
